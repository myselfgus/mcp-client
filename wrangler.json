{
  "name": "mcp-client",
  "main": "src/server.ts",
  "compatibility_date": "2025-03-14",
  "compatibility_flags": ["nodejs_compat"],
  "assets": {
    "directory": "dist/client"
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "meta-mcp-db",
      "database_id": "8dc4c0f5-7f82-4ed2-8501-be0b95a4fa38"
    }
  ],
  "kv_namespaces": [
    {
      "binding": "KV",
      "id": "a1afcfa24f9a4cd2a135117098493a49"
    }
  ],
  "dispatch_namespaces": [
    {
      "binding": "DISPATCHER",
      "namespace": "meta-mcp",
      "outbound": {
        "service": "meta-mcp"
      }
    }
  ],
  "services": [
    {
      "binding": "META_MCP",
      "service": "meta-mcp",
      "environment": "production",
      "entrypoint": "MetaMCPRPC"
    }
  ],
  "vars": {
    "HOST": "<PLACEHOLDER_HOST>"
  },
  "durable_objects": {
    "bindings": [
      {
        "name": "MyAgent",
        "class_name": "MyAgent"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["MyAgent"]
    }
  ],
  "observability": {
    "logs": {
      "enabled": true
    }
  }
}
